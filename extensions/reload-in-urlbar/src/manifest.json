{
	"applications": {
		"gecko": {
			"id": "reload-in-urlbar@exe-boss",
			"strict_min_version": "52.0a1"
		}
	},

	"manifest_version":	2,
	"name":	"__MSG_extensionName__",
	"version":	"__ENV_extensionVersion__",
	"author":	"ExE Boss",
	"description":	"__MSG_description__",
	"default_locale":	"en",

	"icons": {
		"16":	"icons/pastel-svg/arrow-rotate-clockwise-225@16px.png",
		"24":	"icons/pastel-svg/arrow-rotate-clockwise-225@24px.png",
		"32":	"icons/pastel-svg/arrow-rotate-clockwise-225@32px.png",
		"48":	"icons/pastel-svg/arrow-rotate-clockwise-225@48px.png",
		"64":	"icons/pastel-svg/arrow-rotate-clockwise-225@64px.png",
		"72":	"icons/pastel-svg/arrow-rotate-clockwise-225@72px.png",
		"96":	"icons/pastel-svg/arrow-rotate-clockwise-225@96px.png",
		"128":	"icons/pastel-svg/arrow-rotate-clockwise-225@128px.png",
		"256":	"icons/pastel-svg/arrow-rotate-clockwise-225.png"
	},

	"background": {
		"scripts":	["background.js"]
	},
	"page_action": {
		"browser_style":	true,
		"default_icon":	"icons/context-fill/reload.svg",
		"default_title":	"__MSG_pageAction_Reload__",
		"show_matches":	[ "<all_urls>" ]
	}
}
